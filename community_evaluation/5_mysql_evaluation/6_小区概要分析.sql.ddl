drop table if exists community_evaluation.community_evaluation_outline_analysis;
create table community_evaluation.community_evaluation_outline_analysis
(
    `id`                          bigint(20) NOT NULL AUTO_INCREMENT COMMENT '主键id',
    `community_id`                varchar(50) DEFAULT NULL COMMENT '小区id',
    `community_name`              varchar(50) DEFAULT NULL COMMENT '小区名称',
    `city_id`                     varchar(50) DEFAULT NULL COMMENT '城市id',
    `city_name`                   varchar(50) DEFAULT NULL COMMENT '城市名称',
    `district_id`                 varchar(50) DEFAULT NULL COMMENT '区域id',
    `district_name`               varchar(50) DEFAULT NULL COMMENT '区域名称',
    `block_id`                    varchar(50) DEFAULT NULL COMMENT '板块id',
    `block_name`                  varchar(50) DEFAULT NULL COMMENT '板块名称',
    `community_building_pic`      varchar(50) DEFAULT NULL COMMENT '小区楼体图',
    `community_gate_pic`          varchar(50) DEFAULT NULL COMMENT '小区门口',
    `community_green_pic`         varchar(50) DEFAULT NULL COMMENT '绿化图',
    `community_coordinate`        varchar(50) DEFAULT NULL COMMENT '小区坐标',
    `community_fence`             varchar(50) DEFAULT NULL COMMENT '小区围栏',
    `price_up_by_3_years`         varchar(50) DEFAULT NULL COMMENT '3年房价涨幅',
    `price_up_win_range`          varchar(50) DEFAULT NULL COMMENT '跑赢的(板块/区域/城市)',
    `price_up_win_rage`           varchar(50) DEFAULT NULL COMMENT '跑赢(板块/区域/城市)的占比',
    `avg_price`                   varchar(50) DEFAULT NULL COMMENT '小区均价',
    `diff_with_max_rate`          varchar(50) DEFAULT NULL COMMENT '板块内最高价小区相比相差%',
    `growth_index`                varchar(50) DEFAULT NULL COMMENT '成长性指数',
    `list_rate_surpass_range`     varchar(50) DEFAULT NULL COMMENT '超越的(板块/区域/城市)',
    `list_rate_surpass_rate`      varchar(50) DEFAULT NULL COMMENT '超越(板块/区域/城市)的占比',
    `activity_index`              varchar(50) DEFAULT NULL COMMENT '活跃度指数',
    `supply_demand_surpass_range` varchar(50) DEFAULT NULL COMMENT '超越的(板块/区域/城市)',
    `supply_demand_surpass_rate`  varchar(50) DEFAULT NULL COMMENT '超越(板块/区域/城市)的占比',
    `sales_index`                 varchar(50) DEFAULT NULL COMMENT '热销度指数',
    `business_facilities_cnt`     varchar(50) DEFAULT NULL COMMENT '2km商业设施数',
    `subway_cnt`                  varchar(50) DEFAULT NULL COMMENT '2km地铁站数',
    `bus_cnt`                     varchar(50) DEFAULT NULL COMMENT '500m内公交站数',
    `hospital_cnt`                varchar(50) DEFAULT NULL COMMENT '2km三甲医院数',
    `hospital_surpass_range`      varchar(50) DEFAULT NULL COMMENT '超越的(板块/区域/城市)',
    `hospital_surpass_rate`       varchar(50) DEFAULT NULL COMMENT '超越(板块/区域/城市)占比',
    `maturity_index`              varchar(50) DEFAULT NULL COMMENT '成熟度指数',
    `quality_facilities`          varchar(50) DEFAULT NULL COMMENT '品质指数设施',
    `timestamp_v`                 datetime DEFAULT NULL COMMENT '数据处理时间',
    PRIMARY KEY (`id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4 COMMENT '小区概要分析';

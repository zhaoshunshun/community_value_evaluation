drop table if exists community_evaluation.community_evaluation_detail;
create table community_evaluation.community_evaluation_detail
(
    `id`                        bigint(20) NOT NULL AUTO_INCREMENT COMMENT '主键id',
    `community_id`              varchar(50) DEFAULT NULL COMMENT '小区id',
    `community_name`            varchar(50) DEFAULT NULL COMMENT '小区名称',
    `city_id`                   varchar(50) DEFAULT NULL COMMENT '城市id',
    `city_name`                 varchar(50) DEFAULT NULL COMMENT '城市名称',
    `community_addr`            varchar(256) DEFAULT NULL COMMENT '小区地址',
    `district_id`               varchar(50) DEFAULT NULL COMMENT '小区区域id',
    `district_name`             varchar(50) DEFAULT NULL COMMENT '小区区域',
    `block_id`                  varchar(50) DEFAULT NULL COMMENT '小区板块id',
    `block_name`                varchar(50) DEFAULT NULL COMMENT '小区板块',
    `community_coordinate`      varchar(50) DEFAULT NULL COMMENT '小区坐标',
    `community_fence`           text DEFAULT NULL COMMENT '小区围栏',
    `community_avg_price`       varchar(50) DEFAULT NULL COMMENT '小区均价',
    `building_cnt`              varchar(50) DEFAULT NULL COMMENT '小区楼栋数',
    `room_cnt`                  varchar(50) DEFAULT NULL COMMENT '小区户数',
    `building_year`             varchar(50) DEFAULT NULL COMMENT '建筑年代',
    `developer_corp`            varchar(50) DEFAULT NULL COMMENT '开发商',
    `property_name`             varchar(50) DEFAULT NULL COMMENT '物业管理公司',
    `property_fee`              varchar(50) DEFAULT NULL COMMENT '物业费',
    `volume_rate`               varchar(10) DEFAULT NULL COMMENT '容积率',
    `green_rate`                varchar(50) DEFAULT NULL COMMENT '绿化率',
    `list_rate`                 varchar(50) DEFAULT NULL COMMENT '挂牌率',
    `demand_rate`               varchar(50) DEFAULT NULL COMMENT '供需比',
    `facilities`                varchar(50) DEFAULT NULL COMMENT '小区设施',
    `facilities_cnt`            varchar(11) DEFAULT NULL COMMENT '小区设施数',
    `facilities_level`          varchar(50) DEFAULT NULL COMMENT '小区设施等级',
    `quality_stars`             varchar(11) DEFAULT NULL COMMENT '小区品质星数',
    `quality_grade`             varchar(50) DEFAULT NULL COMMENT '小区品质等级',
    `layout_1_rate`             varchar(50) DEFAULT NULL COMMENT '小区户型分布_1房占比',
    `layout_2_rate`             varchar(50) DEFAULT NULL COMMENT '小区户型分布_2房占比',
    `layout_3_rate`             varchar(50) DEFAULT NULL COMMENT '小区户型分布_3房占比',
    `layout_4_rate`             varchar(50) DEFAULT NULL COMMENT '小区户型分布_4房占比',
    `layout_other_rate`         varchar(50) DEFAULT NULL COMMENT '小区户型分布_其他占比',
    `layout_1_area`             varchar(50) DEFAULT NULL COMMENT '小区户型分布_1房面积区间',
    `layout_2_area`             varchar(50) DEFAULT NULL COMMENT '小区户型分布_2房面积区间',
    `layout_3_area`             varchar(50) DEFAULT NULL COMMENT '小区户型分布_3房面积区间',
    `layout_4_area`             varchar(50) DEFAULT NULL COMMENT '小区户型分布_4房面积区间',
    `layout_other_area`         varchar(50) DEFAULT NULL COMMENT '小区户型分布_其他面积区间',
    `layout_main`               varchar(50) DEFAULT NULL COMMENT '小区户型分布_主要户型',
    `layout_main_area`          varchar(50) DEFAULT NULL COMMENT '小区户型分布_主要户型面积区间',
    `layout_secondary`          varchar(50) DEFAULT NULL COMMENT '小区户型分布_次要户型',
    `layout_least`              varchar(50) DEFAULT NULL COMMENT '小区户型分布_最少户型',
    `layout_deal_1`             varchar(50) DEFAULT NULL COMMENT '小区热销户型1_户型',
    `layout_deal_1_area`        varchar(50) DEFAULT NULL COMMENT '小区热销户型1_户型面积区间',
    `layout_deal_1_total_price` varchar(50) DEFAULT NULL COMMENT '小区热销户型1_户型总价区间',
    `layout_deal_1_cnt`         varchar(50) DEFAULT NULL COMMENT '小区热销户型1_成交套数',
    `layout_deal_1_avg_area`    varchar(11) DEFAULT NULL COMMENT '小区热销户型1_均面积',
    `layout_deal_1_avg_price`   varchar(11) DEFAULT NULL COMMENT '小区热销户型1_均总价',
    `layout_deal_1_url`         varchar(100) DEFAULT NULL COMMENT '小区热销户型1_图片链接',
    `layout_deal_2`             varchar(50) DEFAULT NULL COMMENT '小区热销户型2_户型',
    `layout_deal_2_area`        varchar(50) DEFAULT NULL COMMENT '小区热销户型2_户型面积区间',
    `layout_deal_2_total_price` varchar(50) DEFAULT NULL COMMENT '小区热销户型2_户型总价区间',
    `layout_deal_2_cnt`         varchar(50) DEFAULT NULL COMMENT '小区热销户型2_近半年成交套数',
    `layout_deal_2_avg_area`    varchar(11) DEFAULT NULL COMMENT '小区热销户型2_均面积',
    `layout_deal_2_avg_price`   varchar(11) DEFAULT NULL COMMENT '小区热销户型2_均总价',
    `layout_deal_2_url`         varchar(100) DEFAULT NULL COMMENT '小区热销户型2_图片链接',
    `leveling_cnt`              varchar(11) DEFAULT NULL COMMENT '小区规划_小区平层数',
    `mult_level_cnt`            varchar(11) DEFAULT NULL COMMENT '小区规划_小区多层数',
    `small_high_level_cnt`      varchar(11) DEFAULT NULL COMMENT '小区规划_小区小高层数',
    `high_level_cnt`            varchar(11) DEFAULT NULL COMMENT '小区规划_小区高层数',
    `volume_rate_percentile_approx`     varchar(50) DEFAULT NULL COMMENT '小区规划_容积率水平',
    `comfort_level`             varchar(50) DEFAULT NULL COMMENT '小区规划_舒适度',
    `parking_num`               varchar(20) DEFAULT NULL COMMENT '车位数',
    `parking_rate`              varchar(20) DEFAULT NULL COMMENT '停车率',
    `building_type`             varchar(50) DEFAULT NULL COMMENT '建筑类型',
    `act_area`                  varchar(20) DEFAULT NULL COMMENT '占地面积',
    `house_years`               INT  DEFAULT NULL  COMMENT '房龄',
    `investment_potential`      varchar(50) DEFAULT NULL COMMENT '投资潜力',
    `comm_tag`                  varchar(50) DEFAULT NULL COMMENT '小区标签(多标签之间以逗号分隔)',
    `timestamp_v`               datetime DEFAULT NULL COMMENT '数据处理时间',
    PRIMARY KEY (`id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4 COMMENT '小区详情表';






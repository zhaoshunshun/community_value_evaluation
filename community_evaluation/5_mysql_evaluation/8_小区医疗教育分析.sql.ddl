drop table if exists community_evaluation.community_evaluation_medical_education_analysis;
create table community_evaluation.community_evaluation_medical_education_analysis
(
    `id`                          bigint(20) NOT NULL AUTO_INCREMENT COMMENT '主键id',
    `community_id`                varchar(50) DEFAULT NULL COMMENT '小区id',
    `community_name`              varchar(50) DEFAULT NULL COMMENT '小区名称',
    `city_id`                     varchar(50) DEFAULT NULL COMMENT '城市id',
    `city_name`                   varchar(50) DEFAULT NULL COMMENT '城市名称',
    `hospital_3_class_cnt`        varchar(50) DEFAULT NULL COMMENT '三甲医院数',
    `hospital_3_class_1`          varchar(50) DEFAULT NULL COMMENT '三甲医院1',
    `hospital_3_class_2`          varchar(50) DEFAULT NULL COMMENT '三甲医院2',
    `hospital_3_class_3`          varchar(50) DEFAULT NULL COMMENT '三甲医院3',
    `hospital_3_class_1_distance` varchar(50) DEFAULT NULL COMMENT '三甲医院1距离',
    `hospital_3_class_2_distance` varchar(50) DEFAULT NULL COMMENT '三甲医院2距离',
    `hospital_3_class_3_distance` varchar(50) DEFAULT NULL COMMENT '三甲医院3距离',
    `hospital_other_cnt`          varchar(50) DEFAULT NULL COMMENT '其他医院数',
    `hospital_other_1`            varchar(50) DEFAULT NULL COMMENT '其他医院1',
    `hospital_other_2`            varchar(50) DEFAULT NULL COMMENT '其他医院2',
    `hospital_other_3`            varchar(50) DEFAULT NULL COMMENT '其他医院3',
    `hospital_other_1_distance`   varchar(50) DEFAULT NULL COMMENT '其他医院1距离',
    `hospital_other_2_distance`   varchar(50) DEFAULT NULL COMMENT '其他医院2距离',
    `hospital_other_3_distance`   varchar(50) DEFAULT NULL COMMENT '其他医院3距离',
    `medical_facility_desc`       varchar(50) DEFAULT NULL COMMENT '医疗配套描述',
    `kindergarten_cnt`            varchar(50) DEFAULT NULL COMMENT '幼儿园数量',
    `kindergarten_1`              varchar(50) DEFAULT NULL COMMENT '幼儿园1',
    `kindergarten_1_distance`     varchar(50) DEFAULT NULL COMMENT '幼儿园1距离',
    `primary_school_cnt`          varchar(50) DEFAULT NULL COMMENT '小学数量',
    `primary_school_1`            varchar(50) DEFAULT NULL COMMENT '小学1',
    `primary_school_1_distance`   varchar(50) DEFAULT NULL COMMENT '小学1距离',
    `middle_school_cnt`           varchar(50) DEFAULT NULL COMMENT '中学数量',
    `middle_school_1`             varchar(50) DEFAULT NULL COMMENT '中学1',
    `middle_school_distiance`     varchar(50) DEFAULT NULL COMMENT '中学1距离',
    `main_primary_school`         varchar(50) DEFAULT NULL COMMENT '重点小学',
    `main_primary_coordinate`     varchar(50) DEFAULT NULL COMMENT '重点小学坐标',
    `education_facility_desc`     varchar(50) DEFAULT NULL COMMENT '周边教育描述',
    `conclusion_desc`             varchar(50) DEFAULT NULL COMMENT '区位评测总结描述',
    `timestamp_v`                 datetime DEFAULT NULL COMMENT '数据处理时间',
    PRIMARY KEY (`id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4 COMMENT '小区医疗教育分析';

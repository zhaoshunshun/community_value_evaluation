drop table if exists community_evaluation.community_evaluation_traffic_life_analysis;
create table community_evaluation.community_evaluation_traffic_life_analysis
(
    `id`                        bigint(20) NOT NULL AUTO_INCREMENT COMMENT '主键id',
    `community_id`              varchar(50) DEFAULT NULL COMMENT '小区id',
    `community_name`            varchar(50) DEFAULT NULL COMMENT '小区名称',
    `city_id`                   varchar(50) DEFAULT NULL COMMENT '城市id',
    `city_name`                 varchar(50) DEFAULT NULL COMMENT '城市名称',
    `subway_station_cnt`        varchar(50) DEFAULT NULL COMMENT '地铁站数',
    `subway_station_1`          varchar(50) DEFAULT NULL COMMENT '地铁站1',
    `subway_station_2`          varchar(50) DEFAULT NULL COMMENT '地铁站2',
    `subway_station_3`          varchar(50) DEFAULT NULL COMMENT '地铁站3',
    `subway_station_1_distance` varchar(50) DEFAULT NULL COMMENT '地铁站1距离',
    `subway_station_2_distance` varchar(50) DEFAULT NULL COMMENT '地铁站2距离',
    `subway_station_3_distance` varchar(50) DEFAULT NULL COMMENT '地铁站3距离',
    `bus_cnt`                   varchar(50) DEFAULT NULL COMMENT '公交线数',
    `bus_1`                     varchar(50) DEFAULT NULL COMMENT '公交1',
    `bus_2`                     varchar(50) DEFAULT NULL COMMENT '公交2',
    `bus_3`                     varchar(50) DEFAULT NULL COMMENT '公交3',
    `bus_1_distance`            varchar(50) DEFAULT NULL COMMENT '公交1距离',
    `bus_2_distance`            varchar(50) DEFAULT NULL COMMENT '公交2距离',
    `bus_3_distance`            varchar(50) DEFAULT NULL COMMENT '公交3距离',
    `traffic_desc`              varchar(50) DEFAULT NULL COMMENT '交通配套描述',
    `mall_cnt`                  varchar(50) DEFAULT NULL COMMENT '商场数量',
    `mall_1`                    varchar(50) DEFAULT NULL COMMENT '商场1',
    `mall_2`                    varchar(50) DEFAULT NULL COMMENT '商场2',
    `mall_3`                    varchar(50) DEFAULT NULL COMMENT '商场3',
    `mall_4`                    varchar(50) DEFAULT NULL COMMENT '商场4',
    `mall_1_distance`           varchar(50) DEFAULT NULL COMMENT '商场1距离',
    `mall_2_distance`           varchar(50) DEFAULT NULL COMMENT '商场2距离',
    `mall_3_distance`           varchar(50) DEFAULT NULL COMMENT '商场3距离',
    `mall_4_distance`           varchar(50) DEFAULT NULL COMMENT '商场4距离',
    `supermarket_cnt`           varchar(50) DEFAULT NULL COMMENT '超市数量',
    `supermarket_1`             varchar(50) DEFAULT NULL COMMENT '超市1',
    `supermarket_2`             varchar(50) DEFAULT NULL COMMENT '超市2',
    `supermarket_3`             varchar(50) DEFAULT NULL COMMENT '超市3',
    `supermarket_4`             varchar(50) DEFAULT NULL COMMENT '超市4',
    `supermarket_1_distance`    varchar(50) DEFAULT NULL COMMENT '超市1距离',
    `supermarket_2_distance`    varchar(50) DEFAULT NULL COMMENT '超市2距离',
    `supermarket_3_distance`    varchar(50) DEFAULT NULL COMMENT '超市3距离',
    `supermarket_4_distance`    varchar(50) DEFAULT NULL COMMENT '超市4距离',
    `catering_cnt`              varchar(50) DEFAULT NULL COMMENT '餐饮数量',
    `catering_1`                varchar(50) DEFAULT NULL COMMENT '餐饮1',
    `catering_2`                varchar(50) DEFAULT NULL COMMENT '餐饮2',
    `catering_3`                varchar(50) DEFAULT NULL COMMENT '餐饮3',
    `catering_4`                varchar(50) DEFAULT NULL COMMENT '餐饮4',
    `catering_1_distance`       varchar(50) DEFAULT NULL COMMENT '餐饮1距离',
    `catering_2_distance`       varchar(50) DEFAULT NULL COMMENT '餐饮2距离',
    `catering_3_distance`       varchar(50) DEFAULT NULL COMMENT '餐饮3距离',
    `catering_4_distance`       varchar(50) DEFAULT NULL COMMENT '餐饮4距离',
    `bank_cnt`                  varchar(50) DEFAULT NULL COMMENT '银行数量',
    `bank_1`                    varchar(50) DEFAULT NULL COMMENT '银行1',
    `bank_2`                    varchar(50) DEFAULT NULL COMMENT '银行2',
    `bank_3`                    varchar(50) DEFAULT NULL COMMENT '银行3',
    `bank_4`                    varchar(50) DEFAULT NULL COMMENT '银行4',
    `bank_1_distance`           varchar(50) DEFAULT NULL COMMENT '银行1距离',
    `bank_2_distance`           varchar(50) DEFAULT NULL COMMENT '银行2距离',
    `bank_3_distance`           varchar(50) DEFAULT NULL COMMENT '银行3距离',
    `bank_4_distance`           varchar(50) DEFAULT NULL COMMENT '银行4距离',
    `consumption_per`           varchar(50) DEFAULT NULL COMMENT '餐饮人均消费金额',
    `diff_with_city`            varchar(50) DEFAULT NULL COMMENT '高于/低于城市人均消费金额',
    `living_facility_desc`      varchar(50) DEFAULT NULL COMMENT '生活配套描述',
    `timestamp_v`               datetime DEFAULT NULL COMMENT '数据处理时间',
    PRIMARY KEY (`id`)
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8mb4 COMMENT '小区生活交通分析';

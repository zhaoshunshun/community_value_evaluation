CREATE TABLE `community_month_report_convenient_info` (
`id` bigint(11) NOT NULL AUTO_INCREMENT COMMENT '主键自增',
`community_id` varchar(20) DEFAULT NULL COMMENT '小区id',
`district_cd` varchar(50) DEFAULT NULL COMMENT '小区区域id',
`block_cd` varchar(50) DEFAULT NULL COMMENT '小区板块id',
`convenient_score` varchar(2) DEFAULT NULL COMMENT '小区便利分数',
`convenient_hight` varchar(20) DEFAULT NULL COMMENT '小区便利分数高于区域均值的项',
`convenient_low` varchar(20) DEFAULT NULL COMMENT '小区便利分数低于区域均值的项',
`traffic_score` varchar(10) DEFAULT NULL COMMENT '交通分数',
`district_traffic_score` varchar(10) DEFAULT NULL COMMENT '区域交通分数',
`traffic_bus_cnt` varchar(10) DEFAULT NULL COMMENT '小区公交站数量2KM',
`traffic_subway_cnt` varchar(10) DEFAULT NULL COMMENT '小区地铁站数量2KM',
`district_traffic_bus_cnt` varchar(10) DEFAULT NULL COMMENT '区域公交站数量2KM',
`district_traffic_subway_cnt` varchar(10) DEFAULT NULL COMMENT '区域地铁站数量2KM',
`traffic_bus_list` text COMMENT '小区周边2KM的公交车站的数量2KM',
`traffic_subway_list` text COMMENT '小区周边2KM的地铁站数量2KM',
`subway_nearby_distince` varchar(30) DEFAULT NULL COMMENT '最近地铁距离',
`education_score` varchar(10) DEFAULT NULL COMMENT '教育分数',
`district_education_score` varchar(10) DEFAULT NULL COMMENT '区域教育分数',
`education_nursery_cnt` varchar(10) DEFAULT NULL COMMENT '小区幼儿园数量3KM',
`education_primary_cnt` varchar(10) DEFAULT NULL COMMENT '小区小学数量3KM',
`education_middle_cnt` varchar(10) DEFAULT NULL COMMENT '小区中学数量3KM',
`district_education_nursery_cnt` varchar(10) DEFAULT NULL COMMENT '区域幼儿园数据量3KM',
`district_education_primary_cnt` varchar(10) DEFAULT NULL COMMENT '区域小学数量3KM',
`district_education_middle_cnt` varchar(10) DEFAULT NULL COMMENT '区域中学数量3KM',
`education_nursery_list` text COMMENT '小区幼儿园list3KM',
`education_primary_list` text COMMENT '小区小学list3KM',
`education_middle_list` text COMMENT '小区中学list3KM',
`hospital_score` varchar(10) DEFAULT NULL COMMENT '医疗分数',
`district_hospital_score` varchar(10) DEFAULT NULL COMMENT '区域医疗分数',
`hospital_cnt` varchar(10) DEFAULT NULL COMMENT '小区医院数量3KM',
`three_hospital_cnt` varchar(10) DEFAULT NULL COMMENT '小区三甲医院数量3KM',
`district_hospital_cnt` varchar(10) DEFAULT NULL COMMENT '区域医院数量3KM',
`district_three_hospital_cnt` varchar(10) DEFAULT NULL COMMENT '区域三甲医院数量3KM',
`hospital_list` text COMMENT '小区医院list3KM',
`three_hospital_list` text COMMENT '小区三甲医院list3KM',
`shopping_score` varchar(10) DEFAULT NULL COMMENT '购物分数',
`district_shopping_score` varchar(10) DEFAULT NULL COMMENT '区域购物分数',
`shopping_supermarket_cnt` varchar(10) DEFAULT NULL COMMENT '小区周围超市数量3KM',
`shopping_mall_cnt` varchar(10) DEFAULT NULL COMMENT '小区周围购物中心数量3KM',
`district_shopping_supermarket_cnt` varchar(10) DEFAULT NULL COMMENT '区域周围超市数量3KM',
`district_shopping_mall_cnt` varchar(10) DEFAULT NULL COMMENT '区域周围购物中心数量3KM',
`shopping_supermarket_list` text COMMENT '小区周围超市数量3KM',
`shopping_mall_list` text COMMENT '小区周围购物中心数量3KM',
`arder_score` varchar(10) DEFAULT NULL COMMENT '休闲分数',
`district_arder_score` varchar(10) DEFAULT NULL COMMENT '区域休闲分数',
`coffee_house_cnt` varchar(10) DEFAULT NULL COMMENT '小区咖啡馆数量3KM',
`greenland_cnt` varchar(10) DEFAULT NULL COMMENT '小区绿地公园数量3KM',
`movie_cnt` varchar(10) DEFAULT NULL COMMENT '小区电影院数量3KM',
`arder_rank` varchar(10) DEFAULT NULL COMMENT '小区休闲配套排序',
`district_coffee_house_cnt` varchar(10) DEFAULT NULL COMMENT '区域咖啡馆数量3KM',
`district_greenland_cnt` varchar(10) DEFAULT NULL COMMENT '区域绿地公园数量3KM',
`district_movie_cnt` varchar(10) DEFAULT NULL COMMENT '区域电影院数量3KM',
`coffee_house_list` text COMMENT '小区咖啡馆数量3KM',
`greenland_list` text COMMENT '小区绿地公园数量3KM',
`movie_list` text COMMENT '小区电影院数量3KM',
`timestamp_v` datetime DEFAULT NULL COMMENT '数据处理时间',
`batch_no` varchar(255) DEFAULT NULL COMMENT '批次号',
`is_del` tinyint(1) DEFAULT '0' COMMENT '删除标识',
`create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
`update_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
`version` int(11) DEFAULT '0' COMMENT '版本',
`remark` varchar(255) DEFAULT '' COMMENT '备注',
PRIMARY KEY (`id`) USING BTREE,
KEY `community_id` (`community_id`) USING BTREE,
KEY `batch_no` (`batch_no`) USING BTREE,
KEY `district_cd` (`district_cd`) USING BTREE,
KEY `block_cd` (`block_cd`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COMMENT='小区便利指数表'
CREATE TABLE `community_month_report_school` (
`id` bigint(11) NOT NULL AUTO_INCREMENT COMMENT '主键自增',

`community_id` varchar(20) DEFAULT NULL COMMENT '小区id',
`school_id` varchar(32) DEFAULT NULL COMMENT '学校id',
`school_fance` longtext COMMENT '学校围栏',
`school_coordinate` varchar(255) DEFAULT NULL COMMENT '学校坐标',
`enrollment_nums` varchar(1000) DEFAULT NULL COMMENT '招生人数',
`community_school_distince` varchar(10) DEFAULT NULL COMMENT '小区学校距离',
`school_desc` text COMMENT '学校简介',
`school_addr` varchar(255) DEFAULT NULL COMMENT '学校地址',
`school_name` varchar(255) DEFAULT NULL COMMENT '学校名称',
`school_enrollment_target` text COMMENT '招生对象',
`school_fee` text DEFAULT NULL COMMENT '学校学费',
`school_type` varchar(20) DEFAULT NULL COMMENT '办学类型（公办/私半）',
`school_segment_grade` varchar(500) DEFAULT NULL COMMENT '阶段等级（幼儿园、小学、中学）',
`school_characteristics` text DEFAULT NULL COMMENT '学校特色',
`school_scope_road` text DEFAULT NULL COMMENT '小区范围路段',
`batch_no` varchar(255) DEFAULT NULL COMMENT '批次号',
`timestamp_v` datetime DEFAULT NULL COMMENT '数据处理时间',
`is_del` tinyint(1) DEFAULT '0' COMMENT '删除标识',
`create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
`update_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
`version` int(11) DEFAULT '0' COMMENT '版本',
`remark` varchar(255) DEFAULT '' COMMENT '备注',
PRIMARY KEY (`id`) USING BTREE,
KEY `batch_no` (`batch_no`) USING BTREE,
KEY `community_id` (`community_id`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 ROW_FORMAT=DYNAMIC COMMENT='小区学校信息表'
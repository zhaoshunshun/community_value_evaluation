
drop table community_evaluation.community_month_report_base_info
CREATE TABLE `community_evaluation.community_month_report_base_info` (
`id` bigint(11) NOT NULL AUTO_INCREMENT COMMENT '主键自增',

`community_id` varchar(20) DEFAULT NULL COMMENT '小区id',
`community_name` varchar(50) DEFAULT NULL COMMENT '小区名称',
`city_cd` varchar(50) DEFAULT NULL COMMENT '城市id',
`city_name` varchar(50) DEFAULT NULL COMMENT '城市名称',
`district_cd` varchar(50) DEFAULT NULL COMMENT '小区区域id',
`district_name` varchar(50) DEFAULT NULL COMMENT '小区区域',
`block_cd` varchar(50) DEFAULT NULL COMMENT '小区板块id',
`block_name` varchar(50) DEFAULT NULL COMMENT '小区板块',
`addr` varchar(256) DEFAULT NULL COMMENT '小区地址',
`coordinate` varchar(50) CHARACTER SET utf32 DEFAULT NULL COMMENT '小区坐标',
`fence` longtext COMMENT '小区围栏',
`avg_price` varchar(20) DEFAULT NULL COMMENT '小区均价',
`ratio_month` varchar(20) DEFAULT NULL COMMENT '均价环比',
`rack_month_six` varchar(10) DEFAULT NULL COMMENT '小区挂牌均价半年涨幅',
`district_rack_month_six` varchar(10) DEFAULT NULL COMMENT '区域均价半年涨幅',
`building_year` varchar(20) DEFAULT NULL COMMENT '建筑年代',
`building_num` varchar(20) DEFAULT NULL COMMENT '楼栋数',
`building_area` varchar(20) DEFAULT NULL COMMENT '占地面积',
`room_num` varchar(20) DEFAULT NULL COMMENT '户数',
`property_type` varchar(32) DEFAULT NULL COMMENT '物业类型',
`property_fee` varchar(32) DEFAULT NULL COMMENT '物业费',
`developers` varchar(255) DEFAULT NULL COMMENT '开发商',
`property_company` varchar(150) DEFAULT NULL COMMENT '物业公司',
`volume_rate` varchar(10) DEFAULT NULL COMMENT '容积率',
`green_rate` varchar(10) DEFAULT NULL COMMENT '绿化率',
`parking_rate` varchar(10) DEFAULT NULL COMMENT '车户比',
`person_div_car_ind` varchar(10) DEFAULT NULL COMMENT '是否人车分流',
`building_age` varchar(10) DEFAULT NULL COMMENT '楼龄',
`elevator_desc` varchar(10) DEFAULT NULL COMMENT '是否有电梯',
`is_living_quility` varchar(10) COMMENT '品质',
`is_rack STRING` varchar(10) COMMENT '是否有挂牌',
`is_deal STRING` varchar(10) COMMENT '是否有成交',
`batch_no` varchar(255) DEFAULT NULL COMMENT '批次号',
`timestamp_v` datetime DEFAULT NULL COMMENT '数据更新时间',
`is_del` tinyint(1) DEFAULT '0' COMMENT '删除标识',
`create_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
`update_time` timestamp NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '更新时间',
`version` int(11) DEFAULT '0' COMMENT '版本',
`remark` varchar(255) DEFAULT '' COMMENT '备注',
PRIMARY KEY (`id`) USING BTREE,
KEY `community_id` (`community_id`) USING BTREE,
KEY `batch_no` (`batch_no`) USING BTREE,
KEY `city_cd` (`city_cd`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8mb4 COMMENT='小区月报-基础信息表'